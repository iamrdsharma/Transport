    <section
      class="viseo-sec-area transpoart-company-bg"
      style="background-image: url(images/ragister-form-bg.jpg)"
    >
      <div class="container">
        <div class="row">
          <div class="col-lg-7 col-md-6">
            <video
              width="100%"
              height="400"
              autoplay="autoplay"
              loop="loop"
              controls
              muted
            >
              <source src="images/any-movers-video.mp4" type="video/mp4" />
              <source src="images/any-movers-video.mp4" type="video/ogg" />
            </video>
          </div>

          <div class="col-lg-5 col-md-6">
            <% if(registered){ %>
            <label class="pop-up"
              >Successfully Registered<br /><a
                href="user-login"
                style="color: white"
                ><--back-to-login</a
              ></label
            >
            <% } %>
            <div class="login-box">
              <div class="login-heading">
                <h2>REGISTER FORM</h2>
              </div>

              <div class="login-form">
                <form action="/user-signup" method="POST">
                  <div class="form_group">
                    <div class="formitem">
                      <label>First Name</label>

                      <input
                        type="text"
                        name="first"
                        class="form-control log_control"
                        placeholder="First Name"
                      />
                    </div>

                    <div class="formitem">
                      <label>Last Name</label>

                      <input
                        type="text"
                        name="last"
                        class="form-control log_control"
                        placeholder="Last Name"
                      />
                    </div>

                    <div class="formitem">
                      <label>Enter Your Email</label>

                      <input
                        type="text"
                        name="email"
                        class="form-control log_control"
                        placeholder="Enter your email ID"
                      />
                    </div>

                    <div class="formitem">
                      <label>Password</label>

                      <input
                        type="password"
                        name="password"
                        class="form-control log_control"
                        placeholder="Password"
                      />
                    </div>

                    <div class="formitem">
                      <label>Confirm Your Password</label>

                      <input
                        type="password"
                        name="confirmPassword"
                        class="form-control log_control"
                        placeholder="Confirm Your Password"
                      />
                    </div>
                  </div>

                  <div class="log_btn">
                    <div class="log_b btn-secondary">
                      <a class="btn_sub"
                        ><input
                          type="submit"
                          value="Submit"
                          style="
                            background-color: #b81226;
                            border: none;
                            color: #fff;
                          "
                      /></a>
                    </div>
                  </div>

                  <div class="remember-area">
                    <p>
                      Already have an account? <a href="user-login">Login</a>
                    </p>
                  </div>
                </form>
              </div>
            </div>
          </div>
